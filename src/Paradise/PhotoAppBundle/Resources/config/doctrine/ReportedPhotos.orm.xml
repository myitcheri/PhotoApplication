<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Paradise\PhotoAppBundle\Entity\ReportedPhotos" table="reported_photos">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="integer" column="user_id" precision="0" scale="0" nullable="false"/>
    <field name="photoId" type="integer" column="photo_id" precision="0" scale="0" nullable="false"/>
    <field name="reason" type="string" column="reason" length="255" precision="0" scale="0" nullable="false"/>
    <field name="status" type="smallint" column="status" precision="0" scale="0" nullable="false"/>
    <field name="datepost" type="datetime" column="datepost" precision="0" scale="0" nullable="false"/>
  </entity>
</doctrine-mapping>

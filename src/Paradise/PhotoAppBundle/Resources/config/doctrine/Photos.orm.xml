<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Paradise\PhotoAppBundle\Entity\Photos" table="photos">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="filename" type="string" column="filename" length="255" precision="0" scale="0" nullable="false"/>
    <field name="photoDesc" type="text" column="photo_desc" precision="0" scale="0" nullable="true"/>
    <field name="orgImg" type="blob" column="org_img" precision="0" scale="0" nullable="false"/>
    <field name="filesize" type="string" column="filesize" length="45" precision="0" scale="0" nullable="false"/>
    <field name="filetype" type="string" column="filetype" length="255" precision="0" scale="0" nullable="false"/>
    <field name="userId" type="integer" column="user_id" precision="0" scale="0" nullable="false"/>
    <field name="datepost" type="datetime" column="datepost" precision="0" scale="0" nullable="false"/>
    <field name="uniqueId" type="string" column="unique_id" length="255" precision="0" scale="0" nullable="false"/>
  </entity>
</doctrine-mapping>
